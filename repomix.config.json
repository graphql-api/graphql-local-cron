{
    "output": {
        "filePath": ".repomix/repomix-output.md",
        "style": "markdown",
        "parsableStyle": true,
        "compress": false,
        "fileSummary": true,
        "directoryStructure": true,
        "removeComments": false,
        "removeEmptyLines": false,
        "topFilesLength": 5,
        "showLineNumbers": false,
        "copyToClipboard": false,
        "includeEmptyDirectories": false
    },
    "include": [
        "**/*"
    ],
    "ignore": {
        "useGitignore": true,
        "useDefaultPatterns": true,
        "customPatterns": [
            "examples/",
            ".github/",
            ".repomix/",
            "node_modules/",
            "*.log",
            "pnpm-workspace.yaml",
            "repomix.config.json",
            "tsconfig.reference.json",
            ".gitignore",
            ".npmignore",
            ".npmrc",
            ".prettierrc",
            ".stackblitzrc",
            "codegen.yaml"
        ]
    },
    "security": {
        "enableSecurityCheck": true
    }
}